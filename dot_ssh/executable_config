Include /Users/jake/.colima/ssh_config

Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa

Host pi
  HostName 192.168.1.229
  User ubuntu
  IdentityFile ~/.ssh/id_rsa
  
Host jakeserver
  HostName 192.168.1.230
  User jake
  IdentityFile ~/.ssh/id_rsa

Host spotshipper
  HostName app.spotshipper.com
  User root
  IdentityFile ~/.ssh/spotshipper
